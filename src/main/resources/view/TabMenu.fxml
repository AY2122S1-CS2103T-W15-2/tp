<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <TabPane fx:id="tabMenu" VBox.vgrow="NEVER" tabClosingPolicy="UNAVAILABLE">
    <Tab fx:id="tab1" text="Contacts">
      <Label text="Contacts" />
      <GridPane fx:id="contactsContainerBox" styleClass="pane-with-border-2" minWidth="340" prefWidth="340" >
        <children>
          <Label fx:id="tab1Name"/>
        </children>
        <columnConstraints>
          <ColumnConstraints hgrow="ALWAYS" minWidth="100.0" percentWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
      </GridPane>

    </Tab>
    <Tab fx:id="tab2" text="Tasks">
      <Label fx:id="tab2Name"/>
    </Tab>
  </TabPane>
</VBox>
